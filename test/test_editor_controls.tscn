[sub_resource type="GDScript" id="GDScript_06h0j"]
script/source = "@tool
extends PanelContainer

signal dropped_data(data: Variant)
signal something_dragging(value: bool)

@export var print_dropped_data := true

func _notification(what: int) -> void:
	match what:
		NOTIFICATION_DRAG_BEGIN:
			something_dragging.emit(true)
		NOTIFICATION_DRAG_END:
			something_dragging.emit(false)

func _can_drop_data(_at_position: Vector2, _data: Variant) -> bool:
	return true
	
func _drop_data(_at_position: Vector2, data: Variant) -> void:
	if print_dropped_data:
		print(data)
	dropped_data.emit(data)
	
"

