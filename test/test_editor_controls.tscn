resource_name = "copied_label"
script/source = "@tool
extends Control

var tween : Tween

func _ready() -> void:
	modulate = Color.TRANSPARENT

func show_and_fade() -> void:
	modulate = Color.WHITE
	if tween: tween.kill()
	tween = create_tween()
	tween.tween_property(self, ^\"modulate\", Color.TRANSPARENT, 0.5)
	
"

